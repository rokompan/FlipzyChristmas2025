{% schema %}
{
  "name": "Flipzy Feature Video",
  "class": "flipzy-section",
  "settings": [
    { "type": "select", "id": "color_scheme", "label": "Color Scheme", "options": [ { "value": "scheme-white", "label": "White" }, { "value": "scheme-cream", "label": "Cream" } ], "default": "scheme-white" },
    { "type": "select", "id": "desktop_layout", "label": "Desktop Layout", "default": "media_right", "options": [ { "value": "media_left", "label": "Video Left" }, { "value": "media_right", "label": "Video Right" } ] },
    { "type": "select", "id": "mobile_layout", "label": "Mobile Layout", "default": "text_first", "options": [ { "value": "text_first", "label": "Text First" }, { "value": "media_first", "label": "Video First" } ] },
    { "type": "text", "id": "heading", "label": "Title", "default": "What is Flipzy?" },
    { "type": "text", "id": "video_url", "label": "Video URL (MP4)" },
    { "type": "image_picker", "id": "fallback_image", "label": "Fallback Image / Poster" }
  ],
  "blocks": [
    {
      "type": "feature_point",
      "name": "Feature Point",
      "settings": [
        {
          "type": "image_picker",
          "id": "custom_icon",
          "label": "Custom Icon (PNG/SVG)",
          "info": "Upload your own icon here. Overrides selection."
        },
        { 
          "type": "select", 
          "id": "icon_type", 
          "label": "Standard Icon", 
          "options": [ 
            { "value": "check", "label": "Checkmark" },
            
            { "value": "puzzle", "label": "Puzzle (Phosphor - Best)" },
            { "value": "puzzle_group", "label": "Puzzle Group (New)" },
            { "value": "puzzle_corner", "label": "Puzzle Corner (New)" },
            { "value": "puzzle_jigsaw", "label": "Puzzle Jigsaw (New)" },
            { "value": "puzzle_1", "label": "Puzzle 1 (Classic)" },
            { "value": "puzzle_2", "label": "Puzzle 2 (Linked Pair)" },
            { "value": "puzzle_3", "label": "Puzzle 3 (4-Grid)" },
            { "value": "puzzle_4", "label": "Puzzle 4 (Rounded)" },
            { "value": "puzzle_5", "label": "Puzzle 5 (Solid/Filled)" },
            { "value": "puzzle_6", "label": "Puzzle 6 (Abstract)" },
            
            { "value": "sticker", "label": "Sticker / Peeling (New)" },
            { "value": "wrench", "label": "Wrench / Tool (New)" },
            { "value": "hammer", "label": "Hammer (New)" },
            { "value": "screwdriver", "label": "Screwdriver (New)" },

            { "value": "cycle", "label": "Cycle / Loop" }, 
            { "value": "ruler", "label": "Ruler" }, 
            { "value": "bulb", "label": "Lightbulb" }, 
            { "value": "star", "label": "Star" },
            { "value": "heart", "label": "Heart" },
            { "value": "truck", "label": "Truck / Shipping" },
            { "value": "shield", "label": "Shield / Secure" },
            { "value": "leaf", "label": "Leaf / Eco" },
            { "value": "gift", "label": "Gift" },
            { "value": "user", "label": "User / Community" },
            { "value": "clock", "label": "Clock / Time" },
            { "value": "box", "label": "Box / Package" },
            { "value": "settings", "label": "Settings / Gear" },
            
            { "value": "chat", "label": "Chat / Message" },
            { "value": "phone", "label": "Phone" },
            { "value": "mail", "label": "Email / Envelope" },
            { "value": "calendar", "label": "Calendar" },
            { "value": "pin", "label": "Map Pin / Location" },
            
            { "value": "card", "label": "Credit Card" },
            { "value": "cart", "label": "Shopping Cart" },
            { "value": "tag", "label": "Tag / Label" },
            { "value": "percent", "label": "Percent / Discount" },
            
            { "value": "thumb", "label": "Thumb Up / Like" },
            { "value": "trophy", "label": "Trophy / Award" },
            { "value": "smile", "label": "Smile / Happy" },
            
            { "value": "sun", "label": "Sun" },
            { "value": "moon", "label": "Moon" },
            { "value": "drop", "label": "Drop / Water" },
            { "value": "fire", "label": "Fire / Hot" },
            { "value": "eye", "label": "Eye / View" },
            { "value": "lock", "label": "Lock / Private" },
            
            { "value": "home", "label": "Home" },
            { "value": "globe", "label": "Globe / World" },
            { "value": "search", "label": "Search" },
            { "value": "play", "label": "Play Button" },
            { "value": "download", "label": "Download" }
          ], 
          "default": "check" 
        },
        { 
          "type": "richtext", 
          "id": "text", 
          "label": "Text", 
          "default": "<p>Feature text here</p>" 
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Flipzy Feature Video",
      "blocks": [
        { "type": "feature_point", "settings": { "icon_type": "puzzle", "text": "<p>8 customizable wooden pieces</p>" } },
        { "type": "feature_point", "settings": { "icon_type": "cycle", "text": "<p>Simple flip action</p>" } },
        { "type": "feature_point", "settings": { "icon_type": "ruler", "text": "<p>Compact design</p>" } }
      ]
    }
  ]
}
{% endschema %}

<div class="flipzy-feature-section {{ section.settings.color_scheme }} layout-desktop-{% if section.settings.desktop_layout == 'media_left' %}left{% else %}right{% endif %} layout-mobile-{{ section.settings.mobile_layout }}">
  <div class="flipzy-feature-inner">
    
    <div class="flipzy-feature-media">
      <div class="flipzy-feature-media-inner">
        {% if section.settings.video_url != blank %}
          <video 
            class="flipzy-feature-video" 
            autoplay muted loop playsinline 
            {% if section.settings.fallback_image != blank %}
              poster="{{ section.settings.fallback_image | image_url: width: 1200 }}"
            {% endif %}
          >
            <source src="{{ section.settings.video_url }}" type="video/mp4">
          </video>
        {% elsif section.settings.fallback_image != blank %}
          <img class="flipzy-feature-image" src="{{ section.settings.fallback_image | image_url: width: 1200 }}" loading="lazy">
        {% endif %}
      </div>
    </div>

    <div class="flipzy-feature-content">
      {% if section.settings.heading != blank %}<h2 class="flipzy-feature-heading">{{ section.settings.heading }}</h2>{% endif %}
      <div class="flipzy-feature-points">
        {% for block in section.blocks %}
          <div class="flipzy-feature-point" {{ block.shopify_attributes }}>
            <div class="flipzy-feature-icon-box">
              {% if block.settings.custom_icon != blank %}
                <img src="{{ block.settings.custom_icon | image_url: width: 64 }}" alt="icon" loading="lazy">
              {% else %}
                {% case block.settings.icon_type %}
                  
                  {% when 'puzzle' %}
                    <svg style="stroke-width: 16px !important;" viewBox="0 0 256 256" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" xmlns="http://www.w3.org/2000/svg">
                      <path d="M232,112H206.86a24,24,0,0,0-45.72,0H144V94.86a24,24,0,0,0,0-45.72V24a8,8,0,0,0-8-8H48a8,8,0,0,0-8-8V128a8,8,0,0,0,8,8H70.86a24,24,0,0,0,45.72,0H128v17.14a24,24,0,0,0,0,45.72V224a8,8,0,0,0,8,8h88a8,8,0,0,0,8-8V120A8,8,0,0,0,232,112Z"></path>
                    </svg>

                  {% when 'puzzle_group' %}
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M14 8h-4v4h4v-4z"/><path d="M10 8h-4v4h4v-4z"/><path d="M14 12h-4v4h4v-4z"/><path d="M10 12h-4v4h4v-4z"/><path d="M7 7h10v10h-10z"/></svg>
                  {% when 'puzzle_corner' %}
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M20 10c-2 0-3-1-3-3s1-3 3-3v6z"/><path d="M4 14c2 0 3 1 3 3s-1 3-3 3v-6z"/><path d="M14 4c0 2 1 3 3 3s3-1 3-3h-6z"/><path d="M10 20c0-2-1-3-3-3s-3 1-3 3h6z"/><rect x="4" y="4" width="16" height="16" rx="2"/></svg>
                  {% when 'puzzle_jigsaw' %}
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M12 2a3 3 0 0 0-3 3v1H5a2 2 0 0 0-2 2v4a3 3 0 0 0 3 3h1v4a2 2 0 0 0 2 2h4a3 3 0 0 0 3-3v-1h4a2 2 0 0 0 2-2v-4a3 3 0 0 0-3-3h-1V5a2 2 0 0 0-2-2h-4z"/></svg>

                  {% when 'wrench' %}
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M14.7 6.3a1 1 0 0 0 0 1.4l1.6 1.6a1 1 0 0 0 1.4 0l3.77-3.77a6 6 0 0 1-7.94 7.94l-6.91 6.91a2.12 2.12 0 0 1-3-3l6.91-6.91a6 6 0 0 1 7.94-7.94l-3.76 3.76z"/></svg>
                  {% when 'hammer' %}
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M15 12l-8.5 8.5c-.83.83-2.17.83-3 0 0 0 0 0 0 0a2.12 2.12 0 0 1 0-3L12 9"/><path d="M17.64 6.36l-3.64 3.64a2 2 0 0 0 0 2.83L14 12.83a2 2 0 0 0 2.83 0l3.64-3.64a2 2 0 0 0 0-2.83l-.83-.83a2 2 0 0 0-2.83 0z"/></svg>
                  {% when 'screwdriver' %}
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M6 15l-3 3 3 3 3-3-3-3z"/><path d="M9 12l8.5-8.5a2.12 2.12 0 0 1 3 3L12 15"/></svg>
                  
                  {% when 'sticker' %}
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M15.5 3H5a2 2 0 0 0-2 2v14c0 1.1.9 2 2 2h14a2 2 0 0 0 2-2V8.5L15.5 3Z"/><path d="M15 3v6h6"/></svg>

                  {% when 'puzzle_1' %}
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M19.439 15.424a1 1 0 0 0-1.025.99c0 .605.342 1.586.342 1.586.683 1.93-1.082 3.69-3.007 3s-1.442-2.502-3.483-2.502-2.31 1.767-2.31 1.767c-.66 2.05-3.05 1.536-3.71.696-.66-.84-.342-2.486-.342-2.486a1 1 0 0 0-1.025-.99 1 1 0 0 0-1.025.99c0 .605.342 2.652.342 2.652 1.346 3.86 6.22 4.545 7.562 2.652 1.342 1.893 6.216 1.208 7.562-2.652 0 0 .342-2.047.342-2.652a1 1 0 0 0-1.025-.99z"/><path d="M14.5 11a2.5 2.5 0 1 0 0-5 2.5 2.5 0 0 0 0 5z"/></svg>
                  
                  {% when 'puzzle_2' %}
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M10 13a5 5 0 0 0 7.54.54l3-3a5 5 0 0 0-7.07-7.07l-1.72 1.71"></path><path d="M14 11a5 5 0 0 0-7.54-.54l-3 3a5 5 0 0 0 7.07 7.07l1.71-1.71"></path></svg>

                  {% when 'puzzle_3' %}
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><rect x="3" y="3" width="7" height="7"></rect><rect x="14" y="3" width="7" height="7"></rect><rect x="14" y="14" width="7" height="7"></rect><rect x="3" y="14" width="7" height="7"></rect></svg>

                  {% when 'puzzle_4' %}
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M12 22s8-4 8-10V5l-8-3-8 3v7c0 6 8 10 8 10z"></path><path d="M8 11h8"></path><path d="M12 7v8"></path></svg> 
                    
                  {% when 'puzzle_5' %}
                    <svg viewBox="0 0 24 24" fill="currentColor" stroke="none"><path d="M19.5 14.5h-1a1.5 1.5 0 0 1-1.5-1.5v-1a2.5 2.5 0 0 0-5 0v1a1.5 1.5 0 0 1-1.5 1.5h-1A2.5 2.5 0 0 0 7 17v2a.5.5 0 0 0 .5.5h2a1.5 1.5 0 0 1 1.5 1.5v1a2.5 2.5 0 0 0 5 0v-1a1.5 1.5 0 0 1 1.5-1.5h2a.5.5 0 0 0 .5-.5v-2a2.5 2.5 0 0 0-2.5-2.5z"/></svg>

                  {% when 'puzzle_6' %}
                    <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M20.5 6c-2.61.7-2.61 3.3 0 4c1.93.5 1.68 3-1.21 3.5a8 8 0 1 0-9.29 9.29c.5-2.89 3-2.64 3.5-1.21.7 2.61 3.3 2.61 4 0 1.78-.66 1.78-3.34 0-4-2.61-.7-2.61-3.3 0-4 1.78-.66 1.78-3.34 0-4-.7-2.61-3.3-2.61-4 0z"></path></svg>

                  {% when 'cycle' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M23 4v6h-6"></path><path d="M1 20v-6h6"></path><path d="M3.51 9a9 9 0 0 1 14.85-3.36L23 10M1 14l4.64 4.36A9 9 0 0 0 20.49 15"></path></svg>
                  {% when 'ruler' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M2 12.5a.5.5 0 0 1 .5-.5h21a.5.5 0 0 1 .5.5v9a.5.5 0 0 1-.5.5h-21a.5.5 0 0 1-.5-.5v-9Z"/><path d="M6 12v4"/><path d="M10 12v6"/><path d="M14 12v4"/><path d="M18 12v6"/></svg>
                  {% when 'bulb' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M12 2a7 7 0 0 0-7 7c0 2.38 1.19 4.47 3 5.74V17a2 2 0 0 0 2 2h4a2 2 0 0 0 2-2v-2.26C17.81 13.47 19 11.38 19 9a7 7 0 0 0-7-7z"></path><path d="M9 21h6"></path></svg>
                  {% when 'check' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><polyline points="20 6 9 17 4 12"></polyline></svg>
                  {% when 'star' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><polygon points="12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2"></polygon></svg>
                  {% when 'heart' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M20.84 4.61a5.5 5.5 0 0 0-7.78 0L12 5.67l-1.06-1.06a5.5 5.5 0 0 0-7.78 7.78l1.06 1.06L12 21.23l7.78-7.78 1.06-1.06a5.5 5.5 0 0 0 0-7.78z"></path></svg>
                  {% when 'truck' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><rect x="1" y="3" width="15" height="13"></rect><polygon points="16 8 20 8 23 11 23 16 16 16 16 8"></polygon><circle cx="5.5" cy="18.5" r="2.5"></circle><circle cx="18.5" cy="18.5" r="2.5"></circle></svg>
                  {% when 'shield' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M12 22s8-4 8-10V5l-8-3-8 3v7c0 6 8 10 8 10z"></path></svg>
                  {% when 'leaf' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M11 20A7 7 0 0 1 9.8 6.1C15.5 5 17 4.48 19 2c1 2 2 4.18 2 8 0 5.5-4.78 10-10 10Z"/><path d="M2 21c0-3 1.85-5.36 5.08-6C9.5 14.52 12 13 13 12"/></svg>
                  {% when 'gift' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><polyline points="20 12 20 22 4 22 4 12"></polyline><rect x="2" y="7" width="20" height="5"></rect><line x1="12" y1="22" x2="12" y2="7"></line><path d="M12 7H7.5a2.5 2.5 0 0 1 0-5C11 2 12 7 12 7z"></path><path d="M12 7h4.5a2.5 2.5 0 0 0 0-5C13 2 12 7 12 7z"></path></svg>
                  {% when 'user' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M20 21v-2a4 4 0 0 0-4-4H8a4 4 0 0 0-4 4v2"></path><circle cx="12" cy="7" r="4"></circle></svg>
                  {% when 'clock' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><circle cx="12" cy="12" r="10"></circle><polyline points="12 6 12 12 16 14"></polyline></svg>
                  {% when 'box' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M21 16V8a2 2 0 0 0-1-1.73l-7-4a2 2 0 0 0-2 0l-7 4A2 2 0 0 0 3 8v8a2 2 0 0 0 1 1.73l7 4a2 2 0 0 0 2 0l7-4A2 2 0 0 0 21 16z"></path><polyline points="3.27 6.96 12 12.01 20.73 6.96"></polyline><line x1="12" y1="22.08" x2="12" y2="12"></line></svg>
                  {% when 'settings' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><circle cx="12" cy="12" r="3"></circle><path d="M19.4 15a1.65 1.65 0 0 0 .33 1.82l.06.06a2 2 0 0 1 0 2.83 2 2 0 0 1-2.83 0l-.06-.06a1.65 1.65 0 0 0-1.82-.33 1.65 1.65 0 0 0-1 1.51V21a2 2 0 0 1-2 2 2 2 0 0 1-2-2v-.09A1.65 1.65 0 0 0 9 19.4a1.65 1.65 0 0 0-1.82.33l-.06.06a2 2 0 0 1-2.83 0 2 2 0 0 1 0-2.83l.06-.06a1.65 1.65 0 0 0 .33-1.82 1.65 1.65 0 0 0-1.51-1H3a2 2 0 0 1-2-2 2 2 0 0 1 2-2h.09A1.65 1.65 0 0 0 4.6 9a1.65 1.65 0 0 0-.33-1.82l-.06-.06a2 2 0 0 1 0-2.83 2 2 0 0 1 2.83 0l.06.06a1.65 1.65 0 0 0 1.82.33H9a1.65 1.65 0 0 0 1-1.51V3a2 2 0 0 1 2-2 2 2 0 0 1 2 2v.09a1.65 1.65 0 0 0 1 1.51 1.65 1.65 0 0 0 1.82-.33l.06-.06a2 2 0 0 1 2.83 0 2 2 0 0 1 0 2.83l-.06.06a1.65 1.65 0 0 0-.33 1.82V9a1.65 1.65 0 0 0 1.51 1H21a2 2 0 0 1 2 2 2 2 0 0 1-2 2h-.09a1.65 1.65 0 0 0-1.51 1z"></path></svg>
                  
                  {% when 'chat' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M21 11.5a8.38 8.38 0 0 1-.9 3.8 8.5 8.5 0 0 1-7.6 4.7 8.38 8.38 0 0 1-3.8-.9L3 21l1.9-5.7a8.38 8.38 0 0 1-.9-3.8 8.5 8.5 0 0 1 4.7-7.6 8.38 8.38 0 0 1 3.8-.9h.5a8.48 8.48 0 0 1 8 8v.5z"></path></svg>
                  {% when 'phone' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M22 16.92v3a2 2 0 0 1-2.18 2 19.79 19.79 0 0 1-8.63-3.07 19.5 19.5 0 0 1-6-6 19.79 19.79 0 0 1-3.07-8.67A2 2 0 0 1 4.11 2h3a2 2 0 0 1 2 1.72 12.84 12.84 0 0 0 .7 2.81 2 2 0 0 1-.45 2.11L8.09 9.91a16 16 0 0 0 6 6l1.27-1.27a2 2 0 0 1 2.11-.45 12.84 12.84 0 0 0 2.81.7A2 2 0 0 1 22 16.92z"></path></svg>
                  {% when 'mail' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z"></path><polyline points="22,6 12,13 2,6"></polyline></svg>
                  {% when 'calendar' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><rect x="3" y="4" width="18" height="18" rx="2" ry="2"></rect><line x1="16" y1="2" x2="16" y2="6"></line><line x1="8" y1="2" x2="8" y2="6"></line><line x1="3" y1="10" x2="21" y2="10"></line></svg>
                  {% when 'pin' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M21 10c0 7-9 13-9 13s-9-6-9-13a9 9 0 0 1 18 0z"></path><circle cx="12" cy="10" r="3"></circle></svg>
                  
                  {% when 'card' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><rect x="1" y="4" width="22" height="16" rx="2" ry="2"></rect><line x1="1" y1="10" x2="23" y2="10"></line></svg>
                  {% when 'cart' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><circle cx="9" cy="21" r="1"></circle><circle cx="20" cy="21" r="1"></circle><path d="M1 1h4l2.68 13.39a2 2 0 0 0 2 1.61h9.72a2 2 0 0 0 2-1.61L23 6H6"></path></svg>
                  {% when 'tag' %}<svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M20.59 13.41l-7.17 7.17a2 2 0 0 1-2.83 0L2 12V2h10l8.59 8.59a2 2 0 0 1 0 2.82z"></path><line x1="7" y1="7" x2="7.01" y2="7"></line></svg>
