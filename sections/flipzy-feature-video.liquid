<script>document.addEventListener('DOMContentLoaded',function(){const c=document.currentScript.previousElementSibling,v=c.querySelector('.flipzy-feature-video'),i=c.querySelector('.flipzy-feature-image');if(!v)return;const s="{{ section.settings.video_url }}";if(!s)return;function isSlow(){const n=navigator.connection;return n&&(n.saveData||n.effectiveType.includes('2g'))}const l=()=>{if(v.dataset.loaded)return;v.src=s;v.dataset.loaded='true'};if(!isSlow())l();v.addEventListener('loadeddata',()=>{v.style.display='block';if(i)i.classList.add('is-hidden')});});</script>